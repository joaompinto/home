{
    "editor.mouseWheelZoom": true,
    "window.zoomLevel": 0,
    "files.exclude": {
        "**/__pycache__": true,
    },
    "files.watcherExclude": {
        "**/__pycache__": true
    },
    "search.exclude": {
        "**/__pycache__": true
    },
    "editor.minimap.enabled": false,
    "vscode-json-editor.theme": "dark",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.detectIndentation": false,
    }
}